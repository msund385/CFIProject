@model National

    <section id="main-content">
        <section id="wrapper"
            <div class ="row">
                <div class="col-lg-12">
                    <section class="panel">
                        <header class="panel-heading">
                            <div class="col-md-4 col-md-offset-4">
                                <h1>CFI Contact Form </h1>
                            </div>
                        </header>


                    <div id="form"
                   <div class="panel-body">
                       <div class="row">
                           <div class="col-md-4 col-md-offset-1">
                               <div class="form-group">
                                   <asp:Label Text="Your Name" runat="server" />
                                   <asp:TextBox runat="server" Enabled="true" CssClass="form-control input-sm" placeholder="Your Name" />
                                   <br/>
                                   <br />
                                    <asp:Label Text="Your Email" runat="server" />
                                   <asp:Email runat="server" Enabled="true" CssClass="form-control input-sm" placeholder="Your Email" />
                                   <br />
                                   <br />
                                   <asp:Label Text="Your Concern" runat="server" />
                                   <asp:Textbox Rows="5" Columns="30" TextMode="MultiLine" runat="server" Enabled="true" CssClass="form-control input-sm-lg col-lg* "  placeholder="Your Concern" />
                                   <br />
                                   <br />
                                   <asp:Label Text="World Home" runat="server" />
                                   <asp:DropDownList runat="server" CssClass="form-control input-sm">
                                       <asp:ListItem Text="North America" />
                                       <asp:ListItem Text="Europe" />
                                       <asp:ListItem Text="Asia" />
                                       <asp:ListItem Text="Middle East" />
                                       <asp:ListItem Text="Australia" />
                                       <asp:ListItem Text="Africa" />
                                       <asp:ListItem Text="South America" />
                                       <asp:ListItem Text="Western Europe" />

                                   </asp:DropDownList>

                                    <input type="submit" class="btn btn-dark" value="Submit Button" />
                               </div>

                   </div>
                  </div>

              <p class="col-md-2 col-md-offset-1">Thank You for contacting us, have a great day!</p>
